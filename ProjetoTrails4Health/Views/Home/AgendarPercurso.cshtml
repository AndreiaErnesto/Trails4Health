
@{
    ViewData["Title"] = "AgendarPercurso";
}

<h1>AgendarPercurso</h1>
<h2>Mais do que uma Agenda de Espetáculo</h2>
<div>
    <div class="form-group">
        <label asp-for="DataInicio">Data Inicio:</label>
        <input asp-for="DataInicio" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DataFim">Data Fim:</label>
        <input asp-for="DataFim" class="form-control" />
    </div>
    <div class="panel-footer">
        <button type="submit" class="btn btn-primary">Agendar</button> 
    </div>
</div>